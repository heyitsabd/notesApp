<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include('./partials/head') %>
    <title>Login</title>
  </head>
  <body>
    <%- include('./partials/navbar') %>
    <div class="container mt-4">
      <form action="/user/signup" method="post" enctype="multipart/form-data">
        <div class="mb-3">
          <label for="profileImage" class="form-label">Profile Image</label>
          <input
            type="file"
            class="form-control"
            id="profileImage"
            name="profileImage"
            aria-describedby="profileImage"
          />
        </div>
        <div class="mb-3">
          <label for="fullName" class="form-label">Name</label>
          <input
            type="text"
            class="form-control"
            id="fullName"
            name="fullName"
          />
        </div>
        <div class="mb-3">
          <label for="email" class="form-label"
            >Email address</label
          >
          <input
            type="email"
            class="form-control"
            id="email",
            name="email"
            aria-describedby="emailHelp"
          />
        </div>
        <div class="mb-3">
          <label for="password" class="form-label">Password</label>
          <input
            type="password"
            class="form-control"
            id="password"
            name="password"
          />
        </div>
        <button type="submit" class="btn btn-primary">Submit</button>
      </form>
    </div>
  </body>
</html>
